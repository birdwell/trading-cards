{
  "scripts": {
    "start": "node scripts/setup-db.js && concurrently \"BACKEND_PORT=3002 npm run server\" \"PORT=${PORT:-3000} npm run client:start\"",
    "dev": "npm run server:dev & npm run client:dev",
    "server": "npx tsx server/api/server.ts",
    "server:dev": "npx tsx --watch server/api/server.ts",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:start": "cd client && npm run start",
    "build": "npm run client:build && npx tsc",
    "test": "jest",
    "postinstall": "cd client && npm install",
    "db:setup": "node scripts/setup-db.js"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.12",
    "@libsql/client": "^0.15.14",
    "@trpc/client": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "@types/axios": "^0.9.36",
    "@types/node": "^24.3.1",
    "ai": "^5.0.34",
    "axios": "^1.11.0",
    "concurrently": "^9.1.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "exceljs": "^4.4.0",
    "pino": "^9.9.4",
    "pino-pretty": "^13.1.1",
    "playwright": "^1.55.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "zod": "^4.1.5"
  },
  "type": "module",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "drizzle-kit": "^0.31.4",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1"
  }
}
